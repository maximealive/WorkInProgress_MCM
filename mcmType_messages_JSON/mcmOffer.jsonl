// da gemini: per veicolo che deve ottiene la precedenza vedere se riaggiungere advisedTargetRoadResource o
// inserire waypoints
   "basicContainer":{
      "generationDeltaTime":39100,
      "stationID":9001, // ID della RSU
      "stationType":2,  // 2 = Roadside Unit
      "itssRole":0,     // "notAvailable" finché non confermato da tutti
      "position":{
         "latitude":900000001,
         "longitude":1800000001,
         "positionConfidenceEllipse":{
            "semiMajorAxisLength":4095,
            "semiMinorAxisLength":4095,
            "semiMajorAxisOrientation":3601
         },
         "altitude":{
            "altitudeValue":800001,
            "altitudeConfidence":15
         }
      },
      "mcmType":8,      // Offer
      "manoeuvreId":10, // Session ID generato dalla RSU
      "concept":0,      // Agreement Seeking

      // LOGICA PRECEDENZA:
      // Costo 0 indica che per il veicolo 2 è vantaggioso accettare.
      // Se fosse stato il veicolo che doveva fermarsi, il costo sarebbe stato alto (es. 100).
      "rational": {
         "manoeuvreCooperationCost": 0 
      }
   },
   "mcmContainer":{
      "advisedManoeuvreContainer":[
         {
            // DESTINATARIO: Veicolo 2 (quello che vogliamo far passare)
            "executantID":2, 

            // COMANDO SEMANTICO:
            // "Rimani nella corsia e procedi" -> implica non fermarsi/cedere precedenza
            "currentStateAdvisedChange": {
                "stayInLane": null 
            },

            // SUBMANOEUVRES (Obbligatorio per ASN.1 ma svuotato di TRR)
            "submaneuvres":[
               {
                  "submanoeuvreId":1
                  // advisedTargetRoadResource ELIMINATO come richiesto
               }
            ]
         }
      ]
   }
}